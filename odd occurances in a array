// The code finds the unpaired element in an array containing N number of odd integers.
 #include <algorithm>

int solution(vector<int> &A) {
    int l=A.size();
    //first sorts out the array in ascending order
    sort(A.begin(), A.end());
    //then iterates over the array and compare to check the different element
    for(int i=0;i<=l-1;i+=2){
       if(A[i]!=A[i+1]){
         return A[i];
    return A[-1];
    }
}
}
